{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "es6": true,
    "node": true,
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-param-reassign": "off",
    "class-methods-use-this": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  }
}
